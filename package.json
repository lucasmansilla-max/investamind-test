  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate:pg",
    "db:fix-sequences": "tsx scripts/fix-sequences.ts",
    "db:migrate": "tsx scripts/run-migration.ts",
    "cap:add:android": "npx cap add android",
    "cap:add:ios": "npx cap add ios",
    "ios": "npx cap sync ios && npx cap run ios",
    "android": "npm run build && npx cap sync android && npx cap run android",
    "android:dev": "cross-env CAPACITOR_LIVE_RELOAD=true npx cap sync android && cross-env CAPACITOR_LIVE_RELOAD=true npx cap run android",
    "dev:android": "tsx scripts/show-local-ip.ts && cross-env CAPACITOR_LIVE_RELOAD=true npx cap sync android && cross-env CAPACITOR_LIVE_RELOAD=true npx cap run android",
    "android:emulator": "cross-env CAPACITOR_USE_EMULATOR=true CAPACITOR_LIVE_RELOAD=true npx cap sync android && cross-env CAPACITOR_USE_EMULATOR=true CAPACITOR_LIVE_RELOAD=true npx cap run android",
    "android:sync": "npx cap sync android"
  },
